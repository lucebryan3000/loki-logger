{
  "uid": "codeswarm-dim-service-name-unknown-servi",
  "title": "unknown_service triage",
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "30s",
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "editable": true,
  "tags": [
    "canonical",
    "codeswarm",
    "dimension",
    "logging",
    "service_name",
    "triage",
    "unknown_service"
  ],
  "panels": [
    {
      "type": "stat",
      "title": "Logs seen (5m)",
      "datasource": {
        "type": "loki",
        "uid": "P8E80F9AEF21F6940"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(count_over_time({service_name=\"unknown_service\"}[5m]))"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Log rate (5m)",
      "datasource": {
        "type": "loki",
        "uid": "P8E80F9AEF21F6940"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate({service_name=\"unknown_service\"}[5m]))"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Error-like rate (5m)",
      "datasource": {
        "type": "loki",
        "uid": "P8E80F9AEF21F6940"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate({service_name=\"unknown_service\"} |~ \"(?i)(error|fail|exception|panic)\" [5m])) or vector(0)"
        }
      ]
    },
    {
      "type": "table",
      "title": "Top log_source in unknown_service (5m)",
      "datasource": {
        "type": "loki",
        "uid": "P8E80F9AEF21F6940"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "topk(15, sum by (log_source) (count_over_time({service_name=\"unknown_service\"}[5m])))"
        }
      ]
    },
    {
      "type": "table",
      "title": "Top filename in unknown_service (5m)",
      "datasource": {
        "type": "loki",
        "uid": "P8E80F9AEF21F6940"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "topk(15, sum by (filename) (count_over_time({service_name=\"unknown_service\",filename=~\".+\"}[5m])))"
        }
      ]
    },
    {
      "type": "table",
      "title": "Top source_type in unknown_service (5m)",
      "datasource": {
        "type": "loki",
        "uid": "P8E80F9AEF21F6940"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "topk(10, sum by (source_type) (count_over_time({service_name=\"unknown_service\",source_type=~\".+\"}[5m])))"
        }
      ]
    },
    {
      "type": "logs",
      "title": "Live logs",
      "datasource": {
        "type": "loki",
        "uid": "P8E80F9AEF21F6940"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "{service_name=\"unknown_service\"}"
        }
      ]
    }
  ]
}
