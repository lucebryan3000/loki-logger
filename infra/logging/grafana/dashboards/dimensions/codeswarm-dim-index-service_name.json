{
  "uid": "codeswarm-dim-index-service-name",
  "title": "CodeSwarm - Dimension Index: service_name",
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "30s",
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "tags": [
    "codeswarm",
    "logging",
    "dimension",
    "service_name"
  ],
  "templating": {
    "list": [
      {
        "name": "service_name",
        "type": "query",
        "datasource": {
          "type": "loki",
          "uid": "P8E80F9AEF21F6940"
        },
        "query": "label_values(service_name)",
        "includeAll": true,
        "allValue": ".+",
        "refresh": 2,
        "current": {
          "text": "All",
          "value": "$__all"
        }
      }
    ]
  },
  "panels": [
    {
      "id": 1,
      "type": "table",
      "title": "Volume by service_name (5m)",
      "datasource": {
        "type": "loki",
        "uid": "P8E80F9AEF21F6940"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (service_name) (rate({service_name=~\".+\"}[5m]))",
          "queryType": "range",
          "direction": "backward",
          "datasource": {
            "type": "loki",
            "uid": "P8E80F9AEF21F6940"
          }
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      }
    },
    {
      "id": 2,
      "type": "table",
      "title": "Errors by service_name (15m)",
      "datasource": {
        "type": "loki",
        "uid": "P8E80F9AEF21F6940"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (service_name) (count_over_time({service_name=~\".+\"} |~ \"(?i)(error|fail|exception|panic)\" [15m]))",
          "queryType": "range",
          "direction": "backward",
          "datasource": {
            "type": "loki",
            "uid": "P8E80F9AEF21F6940"
          }
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      }
    },
    {
      "id": 3,
      "type": "logs",
      "title": "Live logs (selected service_name)",
      "datasource": {
        "type": "loki",
        "uid": "P8E80F9AEF21F6940"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "{service_name=~\"$service_name\"}",
          "datasource": {
            "type": "loki",
            "uid": "P8E80F9AEF21F6940"
          }
        }
      ],
      "gridPos": {
        "h": 12,
        "w": 24,
        "x": 0,
        "y": 8
      }
    }
  ],
  "annotations": {
    "list": []
  }
}
