{
  "uid": "codeswarm-dim-service-name-codeswarm-mcp",
  "title": "CodeSwarm - log_source=codeswarm_mcp",
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "30s",
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "editable": true,
  "tags": [
    "canonical",
    "codeswarm",
    "codeswarm_mcp",
    "dimension",
    "log_source",
    "logging"
  ],
  "panels": [
    {
      "type": "stat",
      "title": "Logs seen (5m)",
      "datasource": {
        "type": "loki",
        "uid": "P8E80F9AEF21F6940"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(count_over_time({log_source=\"codeswarm_mcp\"}[5m]))"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Log rate (5m)",
      "datasource": {
        "type": "loki",
        "uid": "P8E80F9AEF21F6940"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate({log_source=\"codeswarm_mcp\"}[5m]))"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Error-like rate (5m)",
      "datasource": {
        "type": "loki",
        "uid": "P8E80F9AEF21F6940"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate({log_source=\"codeswarm_mcp\"} |~ \"(?i)(error|fail|exception|panic)\" [5m])) or vector(0)"
        }
      ]
    },
    {
      "type": "table",
      "title": "Top mcp_level (5m)",
      "datasource": {
        "type": "loki",
        "uid": "P8E80F9AEF21F6940"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "topk(15, sum by (mcp_level) (count_over_time({log_source=\"codeswarm_mcp\",mcp_level=~\".+\"}[5m])))"
        }
      ]
    },
    {
      "type": "table",
      "title": "Top filenames (5m)",
      "datasource": {
        "type": "loki",
        "uid": "P8E80F9AEF21F6940"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "topk(15, sum by (filename) (count_over_time({log_source=\"codeswarm_mcp\",filename=~\".+\"}[5m])))"
        }
      ]
    },
    {
      "type": "logs",
      "title": "Live logs",
      "datasource": {
        "type": "loki",
        "uid": "P8E80F9AEF21F6940"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "{log_source=\"codeswarm_mcp\"}"
        }
      ]
    }
  ]
}
