{
  "uid": "codeswarm-source-index",
  "title": "Source Index",
  "timezone": "America/Chicago",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "30s",
  "time": {
    "from": "now-12h",
    "to": "now"
  },
  "tags": [
    "audit",
    "canonical",
    "codeswarm",
    "logging",
    "loki"
  ],
  "templating": {
    "list": [
      {
        "name": "log_source",
        "type": "query",
        "datasource": {
          "type": "loki",
          "uid": "P8E80F9AEF21F6940"
        },
        "query": "label_values(log_source)",
        "includeAll": true,
        "allValue": ".+",
        "refresh": 2,
        "current": {
          "text": "All",
          "value": "$__all"
        }
      },
      {
        "name": "service_name",
        "type": "query",
        "datasource": {
          "type": "loki",
          "uid": "P8E80F9AEF21F6940"
        },
        "query": "label_values(service_name)",
        "includeAll": true,
        "multi": true,
        "allValue": ".+",
        "current": {
          "text": "All",
          "value": "$__all"
        }
      },
      {
        "name": "source_type",
        "type": "query",
        "datasource": {
          "type": "loki",
          "uid": "P8E80F9AEF21F6940"
        },
        "query": "label_values(source_type)",
        "includeAll": true,
        "multi": true,
        "allValue": ".+",
        "current": {
          "text": "All",
          "value": "$__all"
        }
      },
      {
        "name": "env",
        "type": "query",
        "datasource": {
          "type": "loki",
          "uid": "P8E80F9AEF21F6940"
        },
        "query": "label_values(env)",
        "includeAll": true,
        "multi": true,
        "allValue": ".+",
        "current": {
          "text": "All",
          "value": "$__all"
        }
      },
      {
        "name": "filename",
        "type": "query",
        "datasource": {
          "type": "loki",
          "uid": "P8E80F9AEF21F6940"
        },
        "query": "label_values(filename)",
        "includeAll": true,
        "multi": true,
        "current": {
          "text": "All",
          "value": "$__all"
        }
      },
      {
        "name": "stack",
        "type": "query",
        "datasource": {
          "type": "loki",
          "uid": "P8E80F9AEF21F6940"
        },
        "query": "label_values(stack)",
        "includeAll": true,
        "multi": true,
        "allValue": ".+",
        "current": {
          "text": "All",
          "value": "$__all"
        }
      },
      {
        "name": "service",
        "type": "query",
        "datasource": {
          "type": "loki",
          "uid": "P8E80F9AEF21F6940"
        },
        "query": "label_values(service)",
        "includeAll": true,
        "multi": true,
        "allValue": ".+",
        "current": {
          "text": "All",
          "value": "$__all"
        }
      },
      {
        "name": "mcp_level",
        "type": "query",
        "datasource": {
          "type": "loki",
          "uid": "P8E80F9AEF21F6940"
        },
        "query": "label_values(mcp_level)",
        "includeAll": true,
        "multi": true,
        "allValue": ".+",
        "current": {
          "text": "All",
          "value": "$__all"
        }
      },
      {
        "name": "log_type",
        "label": "Log Type",
        "type": "custom",
        "hide": 0,
        "query": "all,debug,info,warn,error/fail",
        "includeAll": false,
        "multi": false,
        "current": {
          "selected": true,
          "text": "all",
          "value": ""
        },
        "options": [
          {
            "selected": true,
            "text": "all",
            "value": ""
          },
          {
            "selected": false,
            "text": "debug",
            "value": "|~ \"(?i)\\bdebug\\b\" !~ \"(?i)(error|err|fatal|panic|fail|failed|exit-code|oom|segfault|crash|denied|refused|timeout)\" !~ \"(?i)(warn|warning)\""
          },
          {
            "selected": false,
            "text": "info",
            "value": "|~ \"(?i)\\binfo\\b\" !~ \"(?i)(error|err|fatal|panic|fail|failed|exit-code|oom|segfault|crash|denied|refused|timeout)\" !~ \"(?i)(warn|warning)\""
          },
          {
            "selected": false,
            "text": "warn",
            "value": "|~ \"(?i)(warn|warning)\" !~ \"(?i)(error|err|fatal|panic|fail|failed|exit-code|oom|segfault|crash|denied|refused|timeout)\""
          },
          {
            "selected": false,
            "text": "error/fail",
            "value": "|~ \"(?i)(error|err|fatal|panic|fail|failed|exit-code|oom|segfault|crash|denied|refused|timeout)\""
          }
        ]
      }
    ]
  },
  "panels": [
    {
      "type": "text",
      "title": "Source Context",
      "options": {
        "mode": "markdown",
        "content": "### Source Context\n- Dashboard scope: multi-source / filter-driven\n- Selected log source filter: `${log_source:text}`\n- Selected service filter: `${service_name:text}`\n- Selected source type filter: `${source_type:text}`\n- Selected file path filter: `${filename:text}`\n- File path values in scope:<br/>`${filename:join:<br/>}`\n- Note: `All` means no extra filter. File paths above are listed one per line for the current filter scope.\n"
      },
      "gridPos": {
        "x": 0,
        "y": 0,
        "w": 24,
        "h": 4
      }
    },
    {
      "id": 1,
      "type": "timeseries",
      "title": "Log volume by source",
      "datasource": {
        "type": "loki",
        "uid": "P8E80F9AEF21F6940"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (log_source) (rate({log_source=~\".+\"}[$__interval]))",
          "queryType": "range",
          "direction": "backward",
          "datasource": {
            "type": "loki",
            "uid": "P8E80F9AEF21F6940"
          },
          "legendFormat": "{{log_source}}"
        }
      ],
      "gridPos": {
        "x": 0,
        "y": 4,
        "w": 12,
        "h": 10
      },
      "fieldConfig": {
        "defaults": {},
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "right",
          "showLegend": true,
          "calcs": [
            "mean",
            "lastNotNull"
          ]
        },
        "tooltip": {
          "mode": "multi",
          "sort": "desc"
        }
      },
      "description": "Shows Log volume by source (5m) for the current dashboard filters and time range. Use this panel to detect anomalies and trends quickly."
    },
    {
      "id": 2,
      "type": "timeseries",
      "title": "Error hits by source",
      "datasource": {
        "type": "loki",
        "uid": "P8E80F9AEF21F6940"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (log_source) (count_over_time({log_source=~\".+\"} |~ \"(?i)(error|fail|exception|panic)\" [$__range]))",
          "queryType": "range",
          "direction": "backward",
          "datasource": {
            "type": "loki",
            "uid": "P8E80F9AEF21F6940"
          },
          "legendFormat": "{{log_source}}"
        }
      ],
      "gridPos": {
        "x": 12,
        "y": 4,
        "w": 12,
        "h": 10
      },
      "fieldConfig": {
        "defaults": {},
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "right",
          "showLegend": true,
          "calcs": [
            "mean",
            "lastNotNull"
          ]
        },
        "tooltip": {
          "mode": "multi",
          "sort": "desc"
        }
      },
      "description": "Shows Error hits by source (15m) for the current dashboard filters and time range. Use this panel to detect anomalies and trends quickly."
    },
    {
      "type": "timeseries",
      "title": "Quiet-source watchdog",
      "datasource": {
        "type": "loki",
        "uid": "P8E80F9AEF21F6940"
      },
      "targets": [
        {
          "refId": "Q",
          "expr": "sort(sum by (log_source) (count_over_time({log_source=~\".+\"}[$__range])))",
          "queryType": "range",
          "legendFormat": "{{log_source}}"
        }
      ],
      "description": "Lowest-volume active sources in the last 15 minutes. Use to catch unexpectedly quiet pipelines.",
      "fieldConfig": {
        "defaults": {},
        "overrides": []
      },
      "gridPos": {
        "x": 0,
        "y": 14,
        "w": 12,
        "h": 10
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "right",
          "showLegend": true,
          "calcs": [
            "mean",
            "lastNotNull"
          ]
        },
        "tooltip": {
          "mode": "multi",
          "sort": "desc"
        }
      }
    },
    {
      "id": 3,
      "type": "logs",
      "title": "Live logs (selected source)",
      "datasource": {
        "type": "loki",
        "uid": "P8E80F9AEF21F6940"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "{log_source=~\"$log_source\"} ${log_type}",
          "datasource": {
            "type": "loki",
            "uid": "P8E80F9AEF21F6940"
          }
        }
      ],
      "gridPos": {
        "x": 0,
        "y": 24,
        "w": 24,
        "h": 12
      },
      "description": "Raw logs for the active filters. Use field filters and Explore to drill into incidents and root causes."
    },
    {
      "type": "timeseries",
      "title": "Logs by volume",
      "datasource": {
        "type": "loki",
        "uid": "P8E80F9AEF21F6940"
      },
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 16
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(count_over_time({log_source=~\"$log_source\"} |~ \"(?i)\\bdebug\\b\" [$__range]))",
          "legendFormat": "debug"
        },
        {
          "refId": "B",
          "expr": "sum(count_over_time({log_source=~\"$log_source\"} |~ \"(?i)\\binfo\\b\" [$__range]))",
          "legendFormat": "info"
        },
        {
          "refId": "C",
          "expr": "sum(count_over_time({log_source=~\"$log_source\"} |~ \"(?i)\\bwarn(ing)?\\b\" [$__range]))",
          "legendFormat": "warn"
        },
        {
          "refId": "D",
          "expr": "sum(count_over_time({log_source=~\"$log_source\"} |~ \"(?i)\\b(err(or)?|fatal|panic|fail(ed)?)\\b\" [$__range]))",
          "legendFormat": "error"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 0,
          "custom": {
            "drawStyle": "bars",
            "lineWidth": 1,
            "fillOpacity": 75,
            "barAlignment": 0,
            "stacking": {
              "mode": "normal",
              "group": "A"
            },
            "spanNulls": false,
            "showPoints": "never"
          }
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "right",
          "showLegend": true,
          "calcs": [
            "mean",
            "lastNotNull"
          ]
        },
        "tooltip": {
          "mode": "multi",
          "sort": "desc"
        }
      },
      "id": 4,
      "description": "Breakdown of log volume by severity (debug/info/warn/error) for the current filters over a 5 minute window."
    },
    {
      "id": 5,
      "type": "bargauge",
      "title": "Top active sources (ranked)",
      "description": "Current busiest log sources by volume.",
      "datasource": {
        "type": "loki",
        "uid": "P8E80F9AEF21F6940"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "topk(12, sum by (log_source) (count_over_time({log_source=~\".+\"}[$__range])))"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 36
      },
      "fieldConfig": {
        "defaults": {},
        "overrides": []
      },
      "options": {
        "orientation": "horizontal",
        "displayMode": "gradient",
        "showUnfilled": false,
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "",
          "values": false
        }
      }
    }
  ],
