      "description": "Expected to be zero when system is healthy/idle. Non-zero indicates actionable anomalies.",
      "gridPos": {
        "x": 0,
        "y": 40,
        "w": 24,
        "h": 12
      }
    },
    {
      "type": "timeseries",
      "title": "Logs by volume",
      "datasource": {
        "type": "loki",
        "uid": "P8E80F9AEF21F6940"
      },
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 32
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(count_over_time({log_source=\"rsyslog_syslog\",syslog_channel=~\"$syslog_channel\",syslog_level=~\"$syslog_level\"} |~ \"(?i)\\bdebug\\b\" [$__range]))",
          "legendFormat": "debug"
        },
        {
          "refId": "B",
          "expr": "sum(count_over_time({log_source=\"rsyslog_syslog\",syslog_channel=~\"$syslog_channel\",syslog_level=~\"$syslog_level\"} |~ \"(?i)\\binfo\\b\" [$__range]))",
          "legendFormat": "info"
        },
        {
          "refId": "C",
          "expr": "sum(count_over_time({log_source=\"rsyslog_syslog\",syslog_channel=~\"$syslog_channel\",syslog_level=~\"$syslog_level\"} |~ \"(?i)\\bwarn(ing)?\\b\" [$__range]))",
          "legendFormat": "warn"
        },
        {
          "refId": "D",
          "expr": "sum(count_over_time({log_source=\"rsyslog_syslog\",syslog_channel=~\"$syslog_channel\",syslog_level=~\"$syslog_level\"} |~ \"(?i)\\b(err(or)?|fatal|panic|fail(ed)?)\\b\" [$__range]))",
          "legendFormat": "error"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 0,
          "custom": {
            "drawStyle": "bars",
            "lineWidth": 1,
            "fillOpacity": 75,
            "barAlignment": 0,
            "stacking": {
              "mode": "normal",
              "group": "A"
            },
            "spanNulls": false,
            "showPoints": "never"
          }
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "right",
          "showLegend": true,
          "calcs": [
            "mean",
            "lastNotNull"
          ]
        },
        "tooltip": {
          "mode": "multi",
          "sort": "desc"
        }
      },
      "id": 1,
      "description": "Breakdown of log volume by severity (debug/info/warn/error) for the current filters over a 5 minute window."
    },
    {
      "id": 2,
      "type": "bargauge",
      "title": "Error-like volume by source (ranked)",
      "description": "Ranked sources by current error-like activity.",
      "datasource": {
        "type": "loki",
        "uid": "P8E80F9AEF21F6940"
