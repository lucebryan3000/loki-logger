{
  "uid": "codeswarm-src-vscode-server",
  "title": "CodeSwarm - vscode_server",
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "30s",
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "tags": [
    "codeswarm",
    "logging",
    "source",
    "vscode_server"
  ],
  "panels": [
    {
      "id": 1,
      "type": "stat",
      "title": "Logs seen (5m)",
      "datasource": {
        "type": "loki",
        "uid": "P8E80F9AEF21F6940"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(count_over_time({log_source=\"vscode_server\"}[5m]))",
          "queryType": "range",
          "direction": "backward",
          "datasource": {
            "type": "loki",
            "uid": "P8E80F9AEF21F6940"
          }
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 0,
        "y": 0
      }
    },
    {
      "id": 2,
      "type": "timeseries",
      "title": "Log rate (5m)",
      "datasource": {
        "type": "loki",
        "uid": "P8E80F9AEF21F6940"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate({log_source=\"vscode_server\"}[5m]))",
          "queryType": "range",
          "direction": "backward",
          "datasource": {
            "type": "loki",
            "uid": "P8E80F9AEF21F6940"
          }
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 18,
        "x": 6,
        "y": 0
      }
    },
    {
      "id": 3,
      "type": "timeseries",
      "title": "Error-like rate (5m)",
      "datasource": {
        "type": "loki",
        "uid": "P8E80F9AEF21F6940"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate({log_source=\"vscode_server\"} |~ \"(?i)(error|fail|exception|panic)\" [5m]))",
          "queryType": "range",
          "direction": "backward",
          "datasource": {
            "type": "loki",
            "uid": "P8E80F9AEF21F6940"
          }
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 8
      }
    },
    {
      "id": 4,
      "type": "logs",
      "title": "Live logs",
      "datasource": {
        "type": "loki",
        "uid": "P8E80F9AEF21F6940"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "{log_source=\"vscode_server\"}",
          "datasource": {
            "type": "loki",
            "uid": "P8E80F9AEF21F6940"
          }
        }
      ],
      "gridPos": {
        "h": 12,
        "w": 24,
        "x": 0,
        "y": 16
      }
    },
    {
      "type": "table",
      "title": "Top services (5m)",
      "datasource": {
        "type": "loki",
        "uid": "P8E80F9AEF21F6940"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "topk(10, sum by (service_name) (count_over_time({log_source=\"vscode_server\", service_name=~\".+\"}[5m])))"
        }
      ]
    },
    {
      "type": "table",
      "title": "Volume by env (5m)",
      "datasource": {
        "type": "loki",
        "uid": "P8E80F9AEF21F6940"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "topk(10, sum by (env) (count_over_time({log_source=\"vscode_server\", env=~\".+\"}[5m])))"
        }
      ]
    },
    {
      "type": "table",
      "title": "Top files (5m)",
      "datasource": {
        "type": "loki",
        "uid": "P8E80F9AEF21F6940"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "topk(10, sum by (filename) (count_over_time({log_source=\"vscode_server\", filename=~\".+\"}[5m])))"
        }
      ]
    },
    {
      "type": "table",
      "title": "Error-like by service (5m)",
      "datasource": {
        "type": "loki",
        "uid": "P8E80F9AEF21F6940"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "topk(10, (sum by (service_name) (rate({log_source=\"vscode_server\"} |~ \"(?i)(error|fail|exception|panic)\" [5m])) or (sum by (service_name) (rate({log_source=\"vscode_server\"}[5m])) * 0)))"
        }
      ]
    }
  ],
  "annotations": {
    "list": []
  },
  "editable": true
}
