{
  "uid": "gpu_overview_v1",
  "title": "CodeSwarm - GPU Telemetry",
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "30s",
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "editable": true,
  "tags": [
    "canonical",
    "codeswarm",
    "gpu",
    "telemetry"
  ],
  "panels": [
    {
      "type": "stat",
      "title": "Telemetry samples (5m)",
      "datasource": {
        "type": "loki",
        "uid": "P8E80F9AEF21F6940"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(count_over_time({log_source=\"telemetry\"}[5m]))"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Telemetry value avg (1m)",
      "datasource": {
        "type": "loki",
        "uid": "P8E80F9AEF21F6940"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "avg_over_time(({log_source=\"telemetry\"} | json | unwrap value)[1m])"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Telemetry value max (5m)",
      "datasource": {
        "type": "loki",
        "uid": "P8E80F9AEF21F6940"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "max_over_time(({log_source=\"telemetry\"} | json | unwrap value)[5m])"
        }
      ]
    },
    {
      "type": "table",
      "title": "Top telemetry sources (5m)",
      "datasource": {
        "type": "loki",
        "uid": "P8E80F9AEF21F6940"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "topk(10, sum by (source) (count_over_time({log_source=\"telemetry\"} | json | source=~\".+\" [5m])))"
        }
      ]
    },
    {
      "type": "logs",
      "title": "Telemetry drill",
      "datasource": {
        "type": "loki",
        "uid": "P8E80F9AEF21F6940"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "{log_source=\"telemetry\"} | json"
        }
      ]
    }
  ]
}
