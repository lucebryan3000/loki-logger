{
  "uid": "gpu_overview_v1",
  "title": "CodeSwarm - GPU Telemetry",
  "timezone": "America/Chicago",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "10s",
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "editable": true,
  "tags": [
    "canonical",
    "codeswarm",
    "gpu",
    "telemetry",
    "nvidia-smi",
    "loki"
  ],
  "description": "Visualization-first GPU telemetry sourced from host nvidia-smi CSV via Alloy -> Loki (log_source=gpu_telemetry). Panels cover utilization, VRAM, temperature, power, and process usage.",
  "templating": {
    "list": [
      {
        "name": "host",
        "type": "query",
        "datasource": {
          "type": "loki",
          "uid": "P8E80F9AEF21F6940"
        },
        "query": "label_values({log_source=\"gpu_telemetry\"}, host)",
        "includeAll": true,
        "multi": true,
        "allValue": ".+",
        "current": {
          "text": "All",
          "value": "$__all"
        }
      },
      {
        "name": "gpu_name",
        "type": "query",
        "datasource": {
          "type": "loki",
          "uid": "P8E80F9AEF21F6940"
        },
        "query": "label_values({log_source=\"gpu_telemetry\",stream=\"gpu\"}, gpu_name)",
        "includeAll": true,
        "multi": true,
        "allValue": ".+",
        "current": {
          "text": "All",
          "value": "$__all"
        }
      }
    ]
  },
  "panels": [
    {
      "type": "stat",
      "title": "GPU telemetry samples (5m)",
      "datasource": {
        "type": "loki",
        "uid": "P8E80F9AEF21F6940"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(count_over_time({log_source=\"gpu_telemetry\",stream=\"gpu\",host=~\"$host\"}[5m]))"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "GPU utilization %",
      "datasource": {
        "type": "loki",
        "uid": "P8E80F9AEF21F6940"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "avg_over_time({log_source=\"gpu_telemetry\",stream=\"gpu\",host=~\"$host\"} | regexp \"^[^,]+,(?P<gpu_name>[^,]+),(?P<gpu_util>[0-9.]+),(?P<memory_pct>[0-9.]+),(?P<memory_used_mib>[0-9.]+),(?P<memory_total_mib>[0-9.]+),(?P<temp_c>[0-9.]+),(?P<power_w>[0-9.]+)$\" | unwrap gpu_util [1m])"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "VRAM usage %",
      "datasource": {
        "type": "loki",
        "uid": "P8E80F9AEF21F6940"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "avg_over_time({log_source=\"gpu_telemetry\",stream=\"gpu\",host=~\"$host\"} | regexp \"^[^,]+,(?P<gpu_name>[^,]+),(?P<gpu_util>[0-9.]+),(?P<memory_pct>[0-9.]+),(?P<memory_used_mib>[0-9.]+),(?P<memory_total_mib>[0-9.]+),(?P<temp_c>[0-9.]+),(?P<power_w>[0-9.]+)$\" | unwrap memory_pct [1m])"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "GPU temperature (C)",
      "datasource": {
        "type": "loki",
        "uid": "P8E80F9AEF21F6940"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "avg_over_time({log_source=\"gpu_telemetry\",stream=\"gpu\",host=~\"$host\"} | regexp \"^[^,]+,(?P<gpu_name>[^,]+),(?P<gpu_util>[0-9.]+),(?P<memory_pct>[0-9.]+),(?P<memory_used_mib>[0-9.]+),(?P<memory_total_mib>[0-9.]+),(?P<temp_c>[0-9.]+),(?P<power_w>[0-9.]+)$\" | unwrap temp_c [1m])"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "GPU power draw (W)",
      "datasource": {
        "type": "loki",
        "uid": "P8E80F9AEF21F6940"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "avg_over_time({log_source=\"gpu_telemetry\",stream=\"gpu\",host=~\"$host\"} | regexp \"^[^,]+,(?P<gpu_name>[^,]+),(?P<gpu_util>[0-9.]+),(?P<memory_pct>[0-9.]+),(?P<memory_used_mib>[0-9.]+),(?P<memory_total_mib>[0-9.]+),(?P<temp_c>[0-9.]+),(?P<power_w>[0-9.]+)$\" | unwrap power_w [1m])"
        }
      ]
    },
    {
      "type": "table",
      "title": "Top GPU processes by memory (expected idle when no compute jobs)",
      "datasource": {
        "type": "loki",
        "uid": "P8E80F9AEF21F6940"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "topk(10, max by (process_name) (max_over_time({log_source=\"gpu_telemetry\",stream=\"proc\",host=~\"$host\"} | regexp \"^[^,]+,[0-9]+,(?P<process_name>[^,]+),(?P<used_gpu_memory_mib>[0-9.]+)$\" | unwrap used_gpu_memory_mib [5m])))"
        }
      ]
    },
    {
      "type": "logs",
      "title": "GPU telemetry drill",
      "datasource": {
        "type": "loki",
        "uid": "P8E80F9AEF21F6940"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "{log_source=\"gpu_telemetry\",host=~\"$host\"}"
        }
      ]
    }
  ],
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": {
          "type": "grafana",
          "uid": "-- Grafana --"
        },
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      },
      {
        "datasource": {
          "type": "grafana",
          "uid": "-- Grafana --"
        },
        "enable": true,
        "hide": false,
        "iconColor": "rgba(255, 140, 0, 1)",
        "name": "Deploy events",
        "target": {
          "limit": 100,
          "matchAny": true,
          "tags": [
            "deploy",
            "release"
          ],
          "type": "tags"
        },
        "type": "dashboard"
      },
      {
        "datasource": {
          "type": "grafana",
          "uid": "-- Grafana --"
        },
        "enable": true,
        "hide": false,
        "iconColor": "rgba(31, 120, 193, 1)",
        "name": "Restart events",
        "target": {
          "limit": 100,
          "matchAny": true,
          "tags": [
            "restart",
            "started",
            "stopped"
          ],
          "type": "tags"
        },
        "type": "dashboard"
      },
      {
        "datasource": {
          "type": "grafana",
          "uid": "-- Grafana --"
        },
        "enable": true,
        "hide": false,
        "iconColor": "rgba(245, 54, 54, 1)",
        "name": "Incident markers",
        "target": {
          "limit": 100,
          "matchAny": true,
          "tags": [
            "incident",
            "error",
            "panic"
          ],
          "type": "tags"
        },
        "type": "dashboard"
      }
    ]
  },
  "graphTooltip": 1
}
