{
  "uid": "loki_health_v1",
  "title": "Loki Health",
  "timezone": "America/Chicago",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "30s",
  "time": {
    "from": "now-12h",
    "to": "now"
  },
  "editable": true,
  "tags": [
    "canonical",
    "codeswarm",
    "health",
    "loki"
  ],
  "panels": [
    {
      "type": "text",
      "title": "Source Context",
      "options": {
        "mode": "markdown",
        "content": "### Source Context\n- Dashboard scope: multi-source / filter-driven\n- Selected log source filter: `${log_source:text}`\n- Selected service filter: `${service_name:text}`\n- Selected source type filter: `${source_type:text}`\n- Selected syslog channel filter: `${syslog_channel:text}`\n- Selected syslog level filter: `${syslog_level:text}`\n- Selected file path filter: `${filename:text}`\n- File path values in scope:<br/>`${filename:join:<br/>}`\n- Note: `All` means no extra filter. File paths above are listed one per line for the current filter scope.\n"
      },
      "gridPos": {
        "x": 0,
        "y": 0,
        "w": 24,
        "h": 4
      }
    },
    {
      "type": "stat",
      "title": "Rsyslog forward/error events",
      "datasource": {
        "type": "loki",
        "uid": "P8E80F9AEF21F6940"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(count_over_time({log_source=\"rsyslog_syslog\",syslog_channel=~\"$syslog_channel\",syslog_level=~\"$syslog_level\"} |~ \"(omfwd|suspend|refused|error)\" [$__range])) or vector(0)"
        }
      ],
      "gridPos": {
        "x": 0,
        "y": 4,
        "w": 6,
        "h": 5
      },
      "description": "Shows Rsyslog forward/error events (30m) for the current dashboard filters and time range. Use this panel to detect anomalies and trends quickly."
    },
    {
      "type": "timeseries",
      "title": "Ingest lines/s",
      "datasource": {
        "type": "prometheus",
        "uid": "PBFA97CFB590B2093"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(loki_distributor_lines_received_total[$__rate_interval]))"
        }
      ],
      "gridPos": {
        "x": 0,
        "y": 9,
        "w": 12,
        "h": 8
      },
      "description": "Shows Ingest lines/s for the current dashboard filters and time range. Use this panel to detect anomalies and trends quickly.",
      "options": {
        "legend": {
          "showLegend": true,
          "displayMode": "table",
          "placement": "right",
          "calcs": [
            "mean",
            "lastNotNull"
          ]
        }
      }
    },
    {
      "type": "timeseries",
      "title": "Ingest bytes/s",
      "datasource": {
        "type": "prometheus",
        "uid": "PBFA97CFB590B2093"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(loki_distributor_bytes_received_total[$__rate_interval]))"
        }
      ],
      "gridPos": {
        "x": 12,
        "y": 9,
        "w": 12,
        "h": 8
      },
      "description": "Shows Ingest bytes/s for the current dashboard filters and time range. Use this panel to detect anomalies and trends quickly.",
      "options": {
        "legend": {
          "showLegend": true,
          "displayMode": "table",
          "placement": "right",
          "calcs": [
            "mean",
            "lastNotNull"
          ]
        }
      }
    },
    {
      "type": "timeseries",
      "title": "Loki request rate/s",
      "datasource": {
        "type": "prometheus",
        "uid": "PBFA97CFB590B2093"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(loki_request_duration_seconds_count[$__rate_interval]))"
        }
      ],
      "gridPos": {
        "x": 0,
        "y": 17,
        "w": 12,
        "h": 8
      },
      "description": "Shows Loki request rate/s for the current dashboard filters and time range. Use this panel to detect anomalies and trends quickly.",
      "options": {
        "legend": {
          "showLegend": true,
          "displayMode": "table",
          "placement": "right",
          "calcs": [
            "mean",
            "lastNotNull"
          ]
        }
      }
    },
    {
      "type": "timeseries",
      "title": "Total log volume",
      "datasource": {
        "type": "loki",
        "uid": "P8E80F9AEF21F6940"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(count_over_time({log_source=~\".+\"}[$__range]))"
        }
      ],
      "gridPos": {
        "x": 12,
        "y": 17,
        "w": 12,
        "h": 8
      },
      "description": "Shows Total log volume (5m) for the current dashboard filters and time range. Use this panel to detect anomalies and trends quickly.",
      "options": {
        "legend": {
          "showLegend": true,
          "displayMode": "table",
          "placement": "right",
          "calcs": [
            "mean",
            "lastNotNull"
          ]
        }
      }
    },
    {
      "type": "timeseries",
      "title": "Top sources by volume",
      "datasource": {
        "type": "loki",
        "uid": "P8E80F9AEF21F6940"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "topk(15, sum by (log_source) (count_over_time({log_source=~\".+\"}[$__range])))",
          "queryType": "range",
          "legendFormat": "{{log_source}}"
        }
      ],
      "gridPos": {
        "x": 0,
        "y": 25,
        "w": 12,
        "h": 10
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "right",
          "showLegend": true,
          "calcs": [
            "mean",
            "lastNotNull"
          ]
        },
        "tooltip": {
          "mode": "multi",
          "sort": "desc"
        }
      },
      "fieldConfig": {
        "defaults": {},
        "overrides": []
      },
      "description": "Top contributors by count for the selected filters and time window. Higher values indicate the primary sources.",
      "transformations": [
        {
          "id": "renameByRegex",
          "options": {
            "regex": "^rsyslog_syslog$",
            "renamePattern": "Syslog (rsyslog)"
          }
        },
        {
          "id": "renameByRegex",
          "options": {
            "regex": "^codeswarm_mcp$",
            "renamePattern": "CodeSwarm MCP"
          }
        },
        {
          "id": "renameByRegex",
          "options": {
            "regex": "^codex_tui$",
            "renamePattern": "Codex TUI"
          }
        },
        {
          "id": "renameByRegex",
          "options": {
            "regex": "^tmux_pane$",
            "renamePattern": "tmux pane"
          }
        },
        {
          "id": "renameByRegex",
          "options": {
            "regex": "^vscode_server$",
            "renamePattern": "VS Code Server"
          }
        },
        {
          "id": "renameByRegex",
          "options": {
            "regex": "^gpu_telemetry$",
            "renamePattern": "GPU Telemetry"
          }
        }
      ]
    },
    {
      "id": 1,
      "type": "bargauge",
      "title": "Loki 5xx by route (ranked)",
      "description": "Top Loki API routes returning server errors.",
      "datasource": {
        "type": "prometheus",
        "uid": "PBFA97CFB590B2093"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "topk(10, sum by (route) (rate(loki_request_duration_seconds_count{status_code=~\"5..\"}[$__rate_interval])))"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 35
      },
      "fieldConfig": {
        "defaults": {},
        "overrides": []
      },
      "options": {
        "orientation": "horizontal",
        "displayMode": "gradient",
        "showUnfilled": false,
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "",
          "values": false
        }
      }
    }
  ],
